scalar Date

type Query {
{{#each entity.queries.singletons}}
{{#eq name "getById"}}
  getById(id: ID, at: Float): {{../entity.className}}
{{else}}
  {{name}}(id: ID, at: Float): {{../entity.className}}
{{/eq}}
{{/each}}
{{#each entity.queries.vectors}}
{{#eq name "getAll"}}
  getAll(at: Float): [{{../entity.className}}]
{{else}}
  {{name}}(id: ID, at: Float): [{{../entity.className}}]
{{/eq}}
{{/each}}
}

type {{entity.className}} {
  id: ID
  {{entity.legacyIdField}}: String
{{#each entity.fields}}
{{#neq cleanName ../entity.legacyIdField}}
  {{cleanName}}: {{graphqlType}}{{#if required}}!{{/if}}
{{/neq}}
{{/each}}
{{#each entity.relationships.parents}}
  {{foreignKeyInChild}}: ID
{{/each}}
{{#each entity.relationships.children}}
  {{fieldName}}: [{{pascalCase targetEntity}}]
{{/each}}
{{#each entity.relationships.parents}}
  {{targetEntity}}: {{pascalCase targetEntity}}
{{/each}}
}
{{#each entity.relationships.children}}

type {{pascalCase targetEntity}} {
  id: ID
}
{{/each}}
